<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/waiting_rfid_background"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sinest.gw_1000.mode.Activity_waiting_rfid">

    <ImageView
        android:id="@+id/activity_waiting_rfid_background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/h_workingmotion0_e"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:weightSum="100">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="7">
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="9"
            android:weightSum="100">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="72">
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="6"
                android:gravity="center|left">

                <ImageView
                    android:id="@+id/waiting_rfid_setting_button"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/setting"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2">
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="13"
                android:gravity="center">

                <TextView
                    android:id="@+id/waiting_rfid_clock"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:alpha="100"
                    android:textSize="40sp"
                    android:format12Hour="HH:MM"
                    android:textColor="#00FFFF"/>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="84"
            android:weightSum="100">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="49"
                android:weightSum="100">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="6.5">
                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="93.5">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:weightSum="100">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="72"
                            android:weightSum="100">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="11">
                            </LinearLayout>

                            <ImageView
                                android:id="@+id/imageView_device_rfid"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="78"
                                android:background="@drawable/open"
                                android:scaleType="fitXY"/>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:id="@+id/waiting_id_rfid"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:weightSum="100">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="33">
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/layout_switch_visible1_rfid"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="27.5"
                            android:weightSum="100">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="28">
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="13">

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center">

                                    <ImageView
                                        android:id="@+id/waiting_rfid_humidity"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        app:srcCompat="@drawable/humidity"/>

                                    <TextView
                                        android:id="@+id/textView_rfid_humidity"
                                        android:layout_width="0dp"
                                        android:layout_height="0dp"
                                        android:alpha="100"
                                        android:text="0"
                                        android:textColor="#000000"
                                        android:textSize="15dp"
                                        android:gravity="center"
                                        android:layout_alignStart="@+id/waiting_rfid_humidity"
                                        android:layout_alignEnd="@+id/waiting_rfid_humidity"
                                        android:layout_alignTop="@+id/waiting_rfid_humidity"
                                        android:layout_alignBottom="@+id/waiting_rfid_humidity"
                                        android:layout_marginRight="15dp"/>

                                </RelativeLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="3">
                            </LinearLayout>

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="12"
                                android:weightSum="100">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="25">
                                </LinearLayout>

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:gravity="center"
                                    android:layout_weight="39">

                                    <ImageView
                                        android:id="@+id/waiting_rfid_temperature_above"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        app:srcCompat="@drawable/temperature"/>

                                    <TextView
                                        android:id="@+id/textView_rfid_temperature_above"
                                        android:layout_width="0dp"
                                        android:layout_height="0dp"
                                        android:layout_alignStart="@+id/waiting_rfid_temperature_above"
                                        android:layout_alignEnd="@+id/waiting_rfid_temperature_above"
                                        android:layout_alignTop="@+id/waiting_rfid_temperature_above"
                                        android:layout_alignBottom="@+id/waiting_rfid_temperature_above"
                                        android:alpha="100"
                                        android:text="0"
                                        android:textColor="#ffffff"
                                        android:gravity="center"/>

                                </RelativeLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="5">
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="13">

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center">

                                    <ImageView
                                        android:id="@+id/waiting_rfid_oxygen"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        app:srcCompat="@drawable/oxygen"/>

                                    <TextView
                                        android:id="@+id/textView_rfid_oxygen"
                                        android:layout_width="0dp"
                                        android:layout_height="0dp"
                                        android:alpha="100"
                                        android:text="0"
                                        android:textColor="#000000"
                                        android:textSize="15dp"
                                        android:gravity="center"
                                        android:layout_alignStart="@+id/waiting_rfid_oxygen"
                                        android:layout_alignEnd="@+id/waiting_rfid_oxygen"
                                        android:layout_alignTop="@+id/waiting_rfid_oxygen"
                                        android:layout_alignBottom="@+id/waiting_rfid_oxygen"
                                        android:layout_marginRight="15dp"/>

                                </RelativeLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="10"
                            android:weightSum="100">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="44">

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="12">

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center">

                                    <ImageView
                                        android:id="@+id/waiting_rfid_temperature_below"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        app:srcCompat="@drawable/temperature"/>

                                    <TextView
                                        android:id="@+id/textView_rfid_temperature_below"
                                        android:layout_width="0dp"
                                        android:layout_height="0dp"
                                        android:layout_alignStart="@+id/waiting_rfid_temperature_below"
                                        android:layout_alignEnd="@+id/waiting_rfid_temperature_below"
                                        android:layout_alignTop="@+id/waiting_rfid_temperature_below"
                                        android:layout_alignBottom="@+id/waiting_rfid_temperature_below"
                                        android:alpha="100"
                                        android:text="0"
                                        android:textColor="#ffffff"
                                        android:gravity="center"/>

                                </RelativeLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="11"
                            android:weightSum="100">

                            <ImageView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:background="@null"
                                android:layout_weight="25"/>

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="50"
                                android:weightSum="100">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="18">
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="32"
                                    android:weightSum="100"
                                    android:orientation="horizontal">

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="32"
                                    android:layout_gravity="center">

                                    <com.sinest.gw_1000.mode.utils.CustomProgressBarBlock
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:id="@+id/seekBar2_rfid"
                                        android:visibility="invisible">
                                    </com.sinest.gw_1000.mode.utils.CustomProgressBarBlock>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/layout_switch_visible3_rfid"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="14"
                            android:weightSum="100">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="27.5">
                            </LinearLayout>

                            <ImageView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="20.5"
                                android:id="@+id/waiting_rfid_dooropen_button"
                                android:background="@drawable/door_open_off"
                                android:scaleType="fitXY"/>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="4">

                            </LinearLayout>

                            <ImageView
                                android:id="@+id/waiting_rfid_doorclose_button"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="20.5"
                                android:scaleType="fitXY"
                                android:background="@drawable/door_close_off"/>

                        </LinearLayout>

                    </LinearLayout>

                </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="51"
                android:weightSum="100">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="45">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:weightSum="100">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="86"
                            android:weightSum="100"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="45">
                            </LinearLayout>

                            <FrameLayout
                                android:id="@+id/frameLayout_rfid_fragment"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:orientation="horizontal"
                                android:layout_weight="40">
                            </FrameLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:weightSum="100">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="86"
                            android:weightSum="100"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="29">
                            </LinearLayout>

                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="64"
                                android:weightSum="100">

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="29">
                                </LinearLayout>

                                <ImageView
                                    android:id="@+id/rfid_card"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="37"
                                    app:srcCompat="@drawable/rfid_card"
                                    android:scaleType="fitXY"/>

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="11">
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="44"
                    android:weightSum="100">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="29"
                        android:weightSum="100">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="45"
                            android:weightSum="100">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="50">
                            </LinearLayout>

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="37.8"
                                android:weightSum="100">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="47"
                                    android:gravity="center|bottom">

                                    <ImageView
                                        android:id="@+id/waiting_rfid_oxygen_up_button"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:background="@drawable/button_up"/>

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="6">
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="47"
                                    android:gravity="center|top">

                                    <ImageView
                                        android:id="@+id/waiting_rfid_oxygen_down_button"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:background="@drawable/button_down"/>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="5">
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="20"
                            android:weightSum="100">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="55">

                                <TextView
                                    android:id="@+id/waiting_rfid_oxygen_text"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:textSize="45dp"
                                    android:text="0"
                                    android:textColor="#00FFFF"
                                    android:gravity="right|bottom"/>
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="29"
                        android:weightSum="100">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="45"
                            android:weightSum="100">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="50">
                            </LinearLayout>

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="40"
                                android:weightSum="100">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="47"
                                    android:gravity="center|bottom">

                                    <ImageView
                                        android:id="@+id/waiting_rfid_pressure_up_button"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:background="@drawable/button_up"/>

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="6">
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="47"
                                    android:gravity="center|top">

                                    <ImageView
                                        android:id="@+id/waiting_rfid_pressure_down_button"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:background="@drawable/button_down"/>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="5">
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="20"
                            android:weightSum="100">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="55">

                                <TextView
                                    android:id="@+id/waiting_rfid_pressure_text"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:textSize="45dp"
                                    android:text="0"
                                    android:textColor="#00FFFF"
                                    android:gravity="right|bottom"/>
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="29"
                        android:weightSum="100">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="45"
                            android:weightSum="100">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="50">
                            </LinearLayout>

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="40"
                                android:weightSum="100">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="47"
                                    android:gravity="center|bottom">

                                    <ImageView
                                        android:id="@+id/waiting_rfid_time_up_button"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:background="@drawable/button_up"/>

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="6">
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="47"
                                    android:gravity="center|top">

                                    <ImageView
                                        android:id="@+id/waiting_rfid_time_down_button"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:background="@drawable/button_down"/>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="5">
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="20"
                            android:weightSum="100">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="55">

                                <TextView
                                    android:id="@+id/waiting_rfid_time_text"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:textSize="45dp"
                                    android:text="0"
                                    android:textColor="#00FFFF"
                                    android:gravity="right|bottom"/>
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>


                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</RelativeLayout>
